{
  "name": "unchecked",
  "description": "A property based test executor",
  "license": "MIT",
  "author": "Sune Simonsen",
  "repository": "git://github.com/unexpectedjs/unchecked.git",
  "version": "2.0.0",
  "main": "./lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "babel --out-dir lib --quiet src",
    "lint": "eslint . && prettier --check '**/*.{js,md}'",
    "lint-staged": "lint-staged",
    "prepare": "npm run build",
    "test": "mocha test README.md"
  },
  "dependencies": {
    "unexpected": "^12.0.0",
    "unexpected-check": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chance-generators": "^3.5.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-config-standard": "^16.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-mocha": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-standard": "^5.0.0",
    "get-stream": "^6.0.0",
    "lint-staged": "^11.0.0",
    "mocha": "^9.0.0",
    "pre-commit": "^1.2.2",
    "prettier": "~2.3.0",
    "prettier-package-json": "^2.0.1",
    "unexpected-markdown": "^5.0.0"
  },
  "keywords": [
    "generative testing",
    "generators",
    "property based testing",
    "quick check",
    "test",
    "testing"
  ],
  "lint-staged": {
    "*.js": [
      "eslint",
      "prettier --write"
    ],
    "package.json": [
      "prettier-package-json --write"
    ]
  },
  "pre-commit": "lint-staged"
}
